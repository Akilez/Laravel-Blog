{
    "name": "fbf/laravel-blog",
    "description": "A Laravel 5 package to add a simple multilingual blog to a site",
    "keywords": ["laravel","blog"],
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "5.*",
        "mayconbordin/rss-l5": "~1.1",
        "fzaninotto/faker": "1.4.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/models",
            "src/seeds"
        ],
        "psr-0": {
            "Fbf\\LaravelBlog": "src/"
        }
    },
    "minimum-stability": "dev"
}
